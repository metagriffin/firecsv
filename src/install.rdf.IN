  <Description about="urn:mozilla:install-manifest">

    <em:id>586de965-3ac7-4f4f-8f39-5ceae75c040f@jetpack</em:id>
    <em:version>{{VERSION}}</em:version>

    <em:name>FireCsv</em:name>
    <em:description>View pretty CSV directly from within Firefox and Thunderbird.</em:description>
    <em:creator>metagriffin</em:creator>
    <em:homepageURL>http://github.com/metagriffin/firecsv</em:homepageURL>
    <!--em:iconURL>chrome://firecsv/skin/logo.png</em:iconURL-->
    <!--em:optionsURL>chrome://firecsv/content/prefs/prefs.xul</em:optionsURL-->

    <em:optionsType>2</em:optionsType>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>false</em:unpack>

    <!-- JSONView used as addon model -->
    <!-- em:contributor>Benjamin Hollis</em:contributor-->

    <!--====================================================================-->

    <!--
     | TODO: it is *INSANELY BROKEN* to have to specify a maxVersion
     | here... hopefully AMO will fix this so that it is no longer
